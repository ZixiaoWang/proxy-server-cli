{
  "compilerOptions": { 
    "target": "es5",
    "module": "commonjs", 
    "lib": [
      "ES2015",
      "ESNext"
    ], 
    "outDir": "dist",
    "strict": true,  
    "moduleResolution": "node", 
    "esModuleInterop": true, 
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true, 
    "forceConsistentCasingInFileNames": true
  }
}
